{
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "es5",
    "lib": ["es2017"],
    "outDir": "lib",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "sourceMap": false
  },
  "include": ["source"]
}
