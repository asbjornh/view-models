{
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "ES5",
    "lib": ["ES2017"],
    "outDir": "lib",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "sourceMap": true
  },
  "include": ["source", "debug.ts"]
}
